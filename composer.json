{
  "name": "lessname/documentor",
  "license": "MIT",
  "require": {
    "php": "^8.4.0",

    "lessname/value-object": "^0.3.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7.0",
    "phpstan/phpstan": "^2.0.0",
    "phpunit/phpunit": "^10.0.0",
    "vimeo/psalm": "^5.0.0"
  },
  "autoload": {
    "psr-4": {
      "LessDocumentor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LessDocumentorTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
